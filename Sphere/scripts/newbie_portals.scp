 return 1
endif
if (tag(realm))
else
 return 1
endif
if (strmatch(<tag.race>,Trpaslik))
 tag(nation,"Ereboran")
 tag(fixnation,1)
elseif (strmatch(<tag.race>,Hobit))
 tag(nation,"Pulcik")
 tag(fixnation,1)
elseif (strmatch(<tag.race>,Elf))
	dialog d_raceclass_nations_fix
elseif (strmatch(<tag.race>,Clovek))
	dialog d_raceclass_nations_fix
elseif (strmatch(<tag.race>,Skret))
	dialog d_raceclass_nations_fix
elseif (strmatch(<tag.race>,Skurut_hai))
 tag(nation,"Mordoran")
 tag(fixnation,1)
elseif (strmatch(<tag.race>,Nurn_man))
	dialog d_raceclass_nations_fix
else 
 sysmessage("Chyba, napis page.")
 return 1
endif
