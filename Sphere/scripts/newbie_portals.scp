[function promazat]
eqlayers(remove)
findlayer(21).remove
findlayer(29).remove
findlayer(31).remove

[function startrace]
if (<strlen(<tag(race)>)>)
  oskin=<?color_<tag(race)>?>
  color=<?color_<tag(race)>?>
  return 0
endif
redMessage("Jeste nemas vybranou rasu.")
return 1

[function startnation]
if (<strlen(<tag(nation)>)>)
  home=<?home_<tag(nation)>?>
  tag(realm,<realm_<tag(nation)>>)
  kills=<eval (tag(realm)-1)*30000)>
  return 0
endif
redMessage("Jeste nemas vybranou narodnost.")
return 1

[function canbeclass]
arg(u,0)
while (arg(u)<ARGVCOUNT)
 if !(strcmpi(<tag.race>,<argv(arg(u))>))
  return 1
 endif
 arg(u,<arg(u)>+1)
endwhile
return 0

[function startclass]
//if (canbeclass(<?races_<args>?>))
tag(level,0)
promazat
givegold(500)
giveitem(i_rune_base)
giveitem(i_crystal_<tag.class>)
newloot(tm_<tag.class>newbie)
class=<args>
findlayer(1).bounce
findlayer(2).bounce
findlayer(2).bounce
eqlayers(color=color_<tag.class>)
startskills


[function startskills]
arg(u,0)
while (arg(u)<50)
	arg(skillname,<findres(skill,<arg(u)>).name>)
	<arg(skillname)>=<safe.base_<tag.class>_<arg(skillname)>>
	arg(u,<arg(u)>+1)
endwhile

