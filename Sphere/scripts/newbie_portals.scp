[DIALOG D_RACEclass_stats BUTTON]
on=0
dialogclose(D_RACEclass_background, 0)

on=2
if (<strlen(<tag(race)>)>)
  if (<strlen(<tag(nation)>)>)
    tag(basestr,<fixNumber(<argtxt(100)>)>)
    tag(basedex,<fixNumber(<argtxt(101)>)>)
    tag(baseintel,<fixNumber(<argtxt(102)>)>)
    tag(basevit,<fixNumber(<argtxt(103)>)>)
    if (<eval tag.basestr>><basestats_<tag.class>_str_max>)||(<eval tag.basestr><<basestats_<tag.class>_str_min>)
      redmessage("Sila neni v danem rozsahu.")
      DIALOG D_RACEclass_stats
      return 0
    elseif (<eval tag.basedex>><basestats_<tag.class>_dex_max>)||(<eval tag.basedex><<basestats_<tag.class>_dex_min>)
      redmessage("Obratnost neni v danem rozsahu.")
      DIALOG D_RACEclass_stats
      return 0
    elseif (<eval tag.baseintel>><basestats_<tag.class>_int_max>)||(<eval tag.baseintel><<basestats_<tag.class>_int_min>)
      redmessage("Inteligence neni v danem rozsahu.")
      DIALOG D_RACEclass_stats
      return 0
    elseif (<eval tag.basevit>><basestats_<tag.class>_vit_max>)||(<eval tag.basevit><<basestats_<tag.class>_vit_min>)
      redmessage("Vitalita neni v danem rozsahu.")
      DIALOG D_RACEclass_stats
      return 0
    elseif (<eval tag(basevit)+tag(baseintel)+tag(basedex)+tag(basestr)>!=300)
      redmessage("Soucet statu musi byt 300, je ale <eval tag(basevit)+tag(baseintel)+tag(basedex)+tag(basestr)>.")
      DIALOG D_RACEclass_stats
      return 0
    endif
    clearboosts
    class(<tag.class>)
    tag.remove(resetting)
    if (startrace)
      return 1
    endif
    if (startnation)
      return 2
    endif
    setplayer
    zakladstats
    zakladabilities
    tag.remove(startdialogpage)
    tag.remove(startdialog)
    tag.remove(resetting)
    accmsg(Chose race #<tag(race)># and profession #<tag(class)># and nation #<tag(nation)>#)
    dialogclose(D_RACEclass_background, 0)
    classmessage("Nyni projdi branou do skutecneho sveta.")
    //novy postavy pote projdou startovni branou i_port_starting, resetujici se postavy ne.
  else
    redMessage("Jeste nemas vybranou narodnost.")
  endif
else
  redmessage("Jeste nemas vybranou rasu.")
  return 0
endif

