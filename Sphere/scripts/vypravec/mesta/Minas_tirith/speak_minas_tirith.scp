dialog(d_banker_ok)
return 1
    
  
ON=1
if !(src.isevent(e_vendor_check))
  SRC.RedMessage("Behem hovoru se nesmis hybat !")
  return 0
endif

if (FINDUID(<src.tag(lastgumpvendor)>).DISTANCE > 6)
  SRC.RedMessage("Jsi prilis daleko od bankere !!")
else
  src.bankself    
  RETURN 1
endif

ON=2
if !(src.isevent(e_vendor_check))
  SRC.RedMessage("Behem hovoru se nesmis hybat !")
  return 0
endif

if (FINDUID(<src.tag(lastgumpvendor)>).DISTANCE > 6)
  SRC.RedMessage("Jsi prilis daleko od bankere !!")
  return 1
endif

arg(sek,<src.findid(i_cashiers_check)>)
if(<arg(sek)>)
  while (<arg(sek)>)
    if (<finduid(<arg(sek)>).topobj>==<src>)
      if (<finduid(<arg(sek)>).more> <= 0)
        finduid(<arg(sek)>).remove
        say("Bohuzel, sek nebyl kryty zadnym zlatem.")
      else
      	if (!(finduid(<arg(sek)>).tag(check_value))&&(<finduid(<arg(sek)>).more> > 10000000))
      		src.redmessage("Tvoj sek ma prilis velku hodnotu a je stary, nemozem ho preplatit. Napis si page a bude vymeneny za novy.")
      		return 0
      	elseif (!(<finduid(<arg(sek)>).more> == <finduid(<arg(sek)>).tag(check_value)>)&&(<finduid(<arg(sek)>).more> > 10000000))
      		src.redmessage("Tvoj sek je bugly, pis page!")
      		src.jail
      		return 0
      	endif
        src.findlayer(29).givegold(<finduid(<arg(sek)>).more>)
      	src.AccMsg("BYL PROPLACEN SEK: uid: <finduid(<arg(sek)>)>, hodnota: <finduid(<arg(sek)>).more> gp.")
        src.sound=037
        src.sysmessage("Do banky bylo vlozeno <finduid(<arg(sek)>).more> gp.")
        finduid(<arg(sek)>).remove
      endif 
    endif
    arg(sek,<src.findid(i_cashiers_check)>)
  endwhile
  say("Rad jsem poslouzil.")
else
  say("Nemas u sebe zadny sek.")
endif
return 1

