  endif
  if(<?src.tag.skill_obchodnictvi?>>299)&&(<?src.tag.skill_obchodnictvi?><600)
    dorand 5
    f_probehl_obchod_skill
    f_probehl_obchod_skill
    enddo
  endif
  endif
  if(<?src.tag.skill_obchodnictvi?>>599)&&(<?src.tag.skill_obchodnictvi?><900)
    dorand 6
    f_probehl_obchod_skill
    enddo
  endif
