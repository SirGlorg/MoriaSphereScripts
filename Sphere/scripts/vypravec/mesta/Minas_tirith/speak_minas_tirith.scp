[FUNCTION f_porter_poznani] // obnovi nepoznani pruvodce
src.ensureTag(hotoveQuesty_2)
src.tag.hotovequesty_2 = <src.tag.hotovequesty_2>&~01
src.tag.hotovequesty_2 = <src.tag.hotovequesty_2>&~040
src.tag.hotovequesty_2 = <src.tag.hotovequesty_2>&~080
src.tag.hotovequesty_2 = <src.tag.hotovequesty_2>&~0100
src.tag.hotovequesty_2 = <src.tag.hotovequesty_2>&~02000
src.tag.hotovequesty_2 = 0

////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////

[FUNCTION f_porter_mt]
if (SRC.TARG.DISTANCE > 3)
  SRC.SYSMESSAGE("Na takovou vzdalenost s tebou jednat nebude.")
  return 1
endif
if (src.tag.realm==2)
  DORAND 4
    say("Jedes?!")
    say("Pomoc, straze!")
    say("Nepriblizuj se ke mne!")
    say("Budu se branit! Zmizni!")
  ENDDO
  return
endif
src.ensureTag(hotoveQuesty_2)
if (<src.tag.hotovequesty_2>&02000)
  say("Nyni uz si nemame co rici. Prijd az s nejakym napadem na udobreni.")
  return
endif
if (<src.tag.hotovequesty_2>&01000) // Pruvodce s tebou nebude dal mluvit, dokud ho nepodplatis
  DIALOG(d_porter_predstavitsemu)
  return
endif
if (<src.tag.hotovequesty_2>&0800) // Pruvodce vi, ze jsi nekromant
  say("Dnes je opravdu tezke vyzit. Neustale se zdrazuje a zdrazuje.")
  DIALOG(d_porter_necro_pozdrav)
  return
endif

if (<src.tag.hotovequesty_2>&01)
  DIALOG(d_porter_mt)
  return
else
  src.say("Prochazis se tu a koukas po mne. Kdo jsi?")
  say("Zdravim a vitam te v Minas Tirith. Kdo jsem? No, na to je jednoducha odpoved - jsem pruvodce.")
  DIALOG(d_porter_poznani)
  return
endif

////////////////////////////////////////////////////////////////////////////////////////////////////////////////////

[FUNCTION f_pani_roouse_mt]
  if (SRC.TARG.DISTANCE > 3)
    SRC.SYSMESSAGE("Na takovou vzdalenost s tebou jednat nebude.")
    return 1
  endif

  if (src.tag.realm==2)
    say("Jedes, potvoro?!")
    return 1
  endif
  src.ensureTag(hotoveQuesty_2)
  if (<src.tag.hotovequesty_2>&010000)
    f_pani_roouse_mt_tfiefornecro
    return 1
  endif
  
  if (<src.tag.hotovequesty_2>&04000)&&(<src.tag.hotovequesty_2>&020000)
    say("Ha, <?src.NAME?>! Tahni ven! V mem podniku uz nemas co pohledavat.")
  elseif (<src.tag.hotovequesty_2>&020000)
    say("Tahni ven! V mem podniku uz nemas co pohledavat.")
  endif
  
  if !(<src.tag.hotovequesty_2>&010000)
    if !(<src.tag.hotovequesty_2>&020000)
      DIALOG(d_pani_roouse_mt)
    endif
  endif
  
///////////////////////////////////////////////////////////

[FUNCTION f_pani_roouse_mt_tfiefornecro]
if (src.profession==class_thief)
  DIALOG(d_pani_roouse_mt_to_thief)
endif
if (src.profession==class_necro)
  DIALOG(d_pani_roouse_mt_to_necro)
endif

