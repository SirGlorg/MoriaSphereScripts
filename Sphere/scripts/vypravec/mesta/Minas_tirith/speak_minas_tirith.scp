    cont.say="No a tak sem ti ho popadl a rikam mu hele mladej tohle je moje postel a moje zena. A on na to.. "
    cont.say="Hehe  chlapy ze ma Roous ale peknou postavu co ? S takovou bych si dal hned rict"
    cont.say="Manwas ty jsi prase vyndej si tu vidlicku z nosu"
    cont.say="Jo jo to uz jsem slysel.."
    cont.say="Hernajs nestrkej do mne"
    cont.say="Tralalala   trala la  la    traaaala la laaalaaa   Tralala la la"
    cont.say="Proc mi to udelala ? Dyt se u mne mela jak v bavlnce"
    cont.say=".. ale na ty hrebiky si musis davat pozor joudo..."
    cont.say=".. no a kdyz si vezmes ze on mel sedm sudu  a ona  tri tak se tam zaziva nemohli vejit.."
    cont.say="hahahaaa   tak to jo tohle.."
    cont.say="hehe hehe"
    cont.say="Jo tak tohle jsem jeste neslysel"
    cont.say=" nepovidej tohle neni mozne..      Kdo  ona jo ?"
    cont.say="...no a pak hodim navnadu potreti...   "
    cont.say="....normalne byla uplne naha.."
    cont.say="..nakonec kdyz uz byly oba vysileni se obratili proti..."
    cont.say="..  kvetiny kvetiny..    ty me nezajimaj   mluv o necem jinem"
    cont.say="..odsoudili ho na tri roky..."
    cont.say="...a pak prej Gandalf stalul v tvari tvar sesti balrogum a vsechny je schodil do propasti... "
    cont.say="..Daster je stejnak hajzl k pohledani..."
  enddo 
  timer=23
return 1

[function f_hosp_anim_snizit_poblilse]
  dorand 3
    cont.f_hosp_anim_poblilse
    f_hosp_anim_tlachani
    f_hosp_anim_tlachani
  enddo

[function f_hosp_anim_poblilse]
  src.say="..dopr..*skit* sem se skalil.. bleeee    *poblilse*"
  src.newitem i_mt_uroous_blitek
  src.act.p=<src.p>
  timer=31

[function f_hosp_anim_informacni]
  dorand 4
    f_hosp_anim_snizit_poblilse
    cont.say="Chlapy slyseli jste o tech premnozenych podkanech v kanalech ?  Radni prej vyhradil urcitou cast vynosu z dani na odmeny lovcum krys"
    cont.say="Zda se ze nam dostali pana Faramira"
    cont.say="Teda uz jste slyseli o tom stracenem archeologovi ?"
  enddo
  timer=17
return 1



