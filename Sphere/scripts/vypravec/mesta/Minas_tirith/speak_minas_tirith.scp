[FUNCTION f_probehl_obchod_gainovani]
  if(<?src.tag.skill_obchodnictvi?><100)
    dorand 3
    f_probehl_obchod_skill
    f_probehl_obchod_skill
    enddo
  endif
  if(<?src.tag.skill_obchodnictvi?>>99)&&(<?src.tag.skill_obchodnictvi?><300)
    dorand 4
    f_probehl_obchod_skill
