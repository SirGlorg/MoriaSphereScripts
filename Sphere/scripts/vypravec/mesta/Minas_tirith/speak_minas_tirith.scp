[function f_manwehoknez]
  if (SRC.TARG.DISTANCE > 3)
    SRC.SYSMESSAGE="Na takovou vzdalenost s tebou jednat nebude"
    return 1
  endif
  if (src.tag.realm==2)
    act.say= "jedes  potvoro !!!!"
    return 1
  endif

  IF (<SRC.BODY> == c_ghost_man)
  //if (SRC.TARG.ISDEAD)
    DIALOG(d_manwehoknez_oziveni)
    return 1
  endif
  if (src.profession==class_necro)
    say= "Odejdi zde nemas co pohledavat ! Toto je stanek zivota a ne smrti !"
    return 1
  endif
  if (src.profession!=class_priest)
    DORAND 3
      say= "Manwe te ochranuj"
      say= "Zdravim synu."
      say= "Pan snami synu"
    ENDDO
    DIALOG(d_manwehoknez)
    return 1
  endif
  IF (<SRC.BODY> == c_man)&&(src.profession==class_priest)
          DORAND 4
      say= "Manwe te ochranuj"
      say= "Zdravim bratre.."
      say= "Pan snami bratre"
      say= "tys bratr <src.name> jestli se nepletu..    Co odemne potrebujes ?"
    ENDDO
           DIALOG(d_manwehoknez)
           return 1
  ELSE (<SRC.BODY> == c_woman)&&(src.profession==class_priest)
    DORAND 4
      say= "Manwe te ochranuj"
      say= "Zdravim sestro.."
      say= "Pan snami sestro"
      say= "tys sestra <src.name> jestli se nepletu. Jen malo zen je takovich jako ty.  Co te privadi ?"
    ENDDO
          DIALOG(d_manwehoknez)
          return 1
  ENDIF

