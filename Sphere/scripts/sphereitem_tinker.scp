  if (src.targ.distance > 2)
    src.sysMessage("Jsi prilis daleko.")
    return 1
  elseif (<eval src.targ.tag(Pickaxing)>) // another pickaxing is already under go
    src.sysMessage("Uz se na tom pracuje.")
    return 1
  elseif (<eval src.targ.tag(PickaxingStop)>) //another pickaxing is manualy blocked until further change on the targetted object
    src.sysMessage("Nevis, co by jsi s tim <src.sex("udelal","udelala")>")
    return 1
  endif
  src.targ.tag(PickaxingSrc,<src>) // sets the last player that is pickacking the target
  if (<src.targ.trigger(@triggerPickaxing)> == 1)) // user denied further action
    return 1
  endif
  src.dismount
  src.anim(9)
  src.targ.tag(Pickaxing,1) // turns on the indicator, which tells that somebody is already working with the target and denies other players to temper with it until the first one is done
  if (<eval src.targ.tag(PickaxingTimer)>)
    arg(pickaxingTimer,<eval src.targ.tag(PickaxingTimer)>)
  else
    arg(pickaxingTimer,2)
  endif
  src.newitemsafe(i_pickaxingDelay)
  lastnew.link=<src.targ>
  src.equip(<lastnew>)
  lastnew.timer=<arg(pickaxingTimer)>
  sfx(0148)
  return 1
endif

