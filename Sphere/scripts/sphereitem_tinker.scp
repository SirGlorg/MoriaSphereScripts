truhla_safe.attr=010
truhla_safe.color=color_o_verite
tag(truhla_id,<arg(truhla_safe)>)

on=@userDclick
f_truhla_heslo_dclick
return 1

on=@targon_char
f_truhla_heslo_targonChar

ON=@TargOn_Ground
src.redmessage("At se divas, jak se divas, na tom miste nikoho nevidis.")
return 1

ON=@TargOn_Item
src.redmessage("Pochybujes, ze uvnitr toho predmetu bude novy majitel truhly.")
return 1

ON=@stackon
src.redmessage("Truhlu musis nejdrive otevrit.")
argo.bounce
return 1

