[ITEMDEF i_rune_tinker]
//recall rune
ID=01f14 //i_rune_marker
NAME=Neaktivni runa
TYPE=T_GEM
VALUE={16 20}
WEIGHT=1.0
RESOURCES=1 i_ingot_mithril, 5 i_gem_ruby, 5 i_gem_citrine, 5 i_gem_amethyst
SKILLMAKE=TINKERING 99.0,t_tinker_tools

CATEGORY=Unique Items
SUBSECTION=Tinker
DESCRIPTION=Neaktivni runa

ON=@userCLICK
src.sysmessage("Tato runa jako by postradala magicke obrazce potrebne pro sve pouziti.")
ON=@userdCLICK
if (<src.Inscription> < 85.0)&&(<SRC.RESTEST 1 i_magicke_pero> )
SRC.SYSMESSAGE("Tve znalosti inskripce nejsou dostatecne.")
elseif (<SRC.RESTEST 1 i_rune_tinker> )&&(<src.Inscription> > 84.9)
SRC.SYSMESSAGE("Nemas nic, cim by se runa dala popsat. Hledej magicke pero, jen ono umozni popsani runy.")
elseif (<SRC.RESTEST 1 i_rune_tinker> )&&(<SRC.RESTEST 1 i_magicke_pero> )&&(<src.Inscription> > 84.9)
src.consume=i_rune_tinker
src.newitem=i_rune_marker
src.act.bounce
ELSE
SRC.SYSMESSAGE("Runu je zrejme nutne necim aktivovat. Je mozne, ze magicka formule napsana na jeji povrch magicke schopnosti ozivi.")
endif

