[function mif_choosemetal]
//var(metal,<metal[<eval {1,300,2,300,5,40,6,30,7,10,8,5,9,1}>]>) // stare nastavenie
var(metal,<metal[<eval {1,500,2,400,5,200,6,100,7,20,8,10,9,1}>]>) // nove nastavenie

[function mif_choosemetal_high]
var(metal,<metal[<eval {5,300,6,200,7,80,8,30,9,1}>]>)

[function mif_choosemetal_superHigh]
var(metal,<metal[<eval {6,200,7,80,8,30,9,1}>]>)

[function mif_chooseitem]
var(itemgroup,<eval {0 2 1 2 2 1}>)//weapon, armor, other
if (<argvcount> > 0)	//jen jistici podminka kdyby to nekdo pouzil bez argumentu
  if (<argv(0)> == 1)
    mif_choosemetal_high
  elseif (<argv(0)> == 2)
    mif_choosemetal_superHigh
  else
    mif_choosemetal
  endif
else
  mif_choosemetal
endif
doswitch <itemgroup>
  mif_chooseweapon
  mif_choosearmor
  mif_chooseother
enddo
return <itembaseid>

[function mif_chooseother]
dorand 8
  mif_chooseDress
  mif_chooseEarrings
  mif_chooseBracelet
  mif_chooseRing
  mif_chooseNecklace
  mif_chooseHat
  var(itembaseid,i_cape)
  var(itembaseid,i_sash)
enddo

[function mif_chooseEarrings]
doswitch {0 60 1 40}
  var(itembaseid,i_earrings_gold)
  var(itembaseid,i_earrings_silver)
enddo

[function mif_chooseBracelet]
doswitch {0 60 1 40}
  var(itembaseid,i_bracelet_gold)
  var(itembaseid,i_bracelet_silver)
enddo

[function mif_chooseRing]
doswitch {0 60 1 40}
  var(itembaseid,i_ring_gold)
  var(itembaseid,i_ring_silver)
enddo

[function mif_chooseNecklace]
doswitch {0 60 1 40}
  var(itembaseid,i_necklace_gold)
  var(itembaseid,i_necklace_silver)
enddo

[function mif_chooseDress]
// robe 50%, plain dress 25%, full dress 25%
doswitch {0 50 1 25 2 25}
  var(itembaseid,i_robe)
  var(itembaseid,i_dress_plain)
  var(itembaseid,i_dress_full)
enddo

[function mif_chooseHat]
dorand 5
  var(itembaseid,i_hat_wizards)
  var(itembaseid,i_hat_feather)
  var(itembaseid,i_hat_straw)
  var(itembaseid,i_hat_floppy)
  var(itembaseid,i_skull_cap)
enddo
 

[function mif_choosearmor]
//plate 23%, chain 23%, ring 18%, studdet 18% bone 18% - podle navrhu pegasky :)
doswitch {0 23 1 23 2 18 3 18 4 18}
	mif_chooseplate
	mif_choosechain
	mif_chooseringmail
	mif_choosestudded
	mif_choosebone
enddo

[function mif_chooseplate]
dorand 7
 var(itembaseid,i_<metal>_HEATER_SHIELD)
 var(itembaseid,i_<metal>_PLATEMAIL_ARMS )
 var(itembaseid,i_<metal>_PLATEMAIL_leggings )
 var(itembaseid,i_<metal>_PLATEMAIL_GORGET )
 var(itembaseid,i_<metal>_PLATEMAIL_GAUNTLETS)
 mif_chooseplatemail
 mif_chooseplatehelm
enddo

[function mif_chooseplatemail]
//mans platemail chest 75%, woman 25%
doswitch {0 75 1 25}
  var(itembaseid,i_<metal>_PLATEMAIL )
  var(itembaseid,i_<metal>_FEMALE_PLATEMAIL)
enddo
 
[function mif_chooseplatehelm]
dorand 4
 var(itembaseid,i_<metal>_PLATE_HELM )
 var(itembaseid,i_<metal>_CLOSED_HELM )
 var(itembaseid,i_<metal>_BASCINET )
 var(itembaseid,i_<metal>_NOSE_HELM)
enddo

[function mif_choosechain]
dorand 7
 var(itembaseid,i_<metal>_METAL_SHIELD )
 var(itembaseid,i_<metal>_CHAINMAIL_COIF  )
 var(itembaseid,i_<metal>_CHAINMAIL_GORGET  )
