 var(itembaseid,i_<metal>_CHAINMAIL_LEGGINGS )
 var(itembaseid,i_<metal>_CHAINMAIL_TUNIC)
enddo

[function mif_chooseringmail]
dorand 7
 var(itembaseid,i_<metal>_kite_wood_shield )
