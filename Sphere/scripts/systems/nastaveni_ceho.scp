  arg(dynamdeftext,<safe.<tag.nastaveni_prefdefaulttype><arg(number)><tag.nastaveni_sufdefaulttype>>)
  arg(vysleddeftext,<tag.nastaveni_prefdefault><arg(dynamdeftext)><tag.nastaveni_sufdefault>)//slozenej nazev default (soucasne) hodnoty
  arg(vysleddef,<?safe.<arg(vysleddeftext)>?>)//soucasna hodnota
  if (strcmpi("<arg(vysleddef)>","<arg(vysledvar)>"))
   if (strlen(<StrGetTok("<arg(vysleddeftext)>",0,".")>)) //delka prvniho argumentu pred teckou je nenulova, tj nemame tady neco jako ".resources(XY)"
    //logmsg(Uplatneni hodnoty:'<arg(vysleddeftext)>(<arg(vysledvar)>)')
    <arg(vysleddeftext)>(<arg(vysledvar)>)
   endif
  endif
 endif
 arg(number,<arg(number)>+1)
endwhile

