   var(nastaveni_armorparts_<argv(2)>_<arg(defname)>,<arg(value)>)
  endif
 else
  //arg(defname,"<arg(defname)>.<argv(2)>")
  //accmsg(huhle)
  if (<?safe <arg(defname)>?>)//dana vec existuje
   //arg(defvalue,<?<arg(defname)>.<argv(2)>?>)
   arg(defproperty,<arg(defname)>.<argv(2)>)
   arg(defvalue,<eval <arg(defproperty)>>)
   arg(defstring,"<?<arg(defproperty)>?>")
