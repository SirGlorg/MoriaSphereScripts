[function f_nastaveni_setdefault]
arg(number,0)
while (arg(number)<51)
 arg(dynamvartext,<safe <tag.nastaveni_prefvaluetype><arg(number)><tag.nastaveni_sufvaluetype>>)
 arg(vysledvar,<safe <tag.nastaveni_prefvalue><arg(dynamvartext)><tag.nastaveni_sufvalue>>)//slozena hodnota
