   endif
  endif
 elseif (<argv(4)>==2)
  if (<?safe.nastaveni_armorparts_<argv(2)>_<arg(defname)>?>!=<?arg(value)?>)
   //smsg nastaveno: var.nastaveni_armorparts_<argv(2)>_<arg(defname)>=<arg(value)>
