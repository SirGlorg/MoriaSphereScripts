[function nastav_townstones]
if(!strcmpi(<baseid>,i_townstone))||(!strcmpi(<baseid>,i_shrine_power))
 region.tag(townstone,<uid>)
endif

[function nastav_generalstones]
if(!strcmpi(<baseid>,i_generalstone))||(!strcmpi(<baseid>,i_lordnazgulsstone))
 region.tag(generalstone,<uid>)
endif

[function nastav_captainstones]
if(!strcmpi(<baseid>,i_captainstone))||(!strcmpi(<baseid>,i_mordorcaptainstone))
 region.tag(keep,<uid>)
endif

[function nastav_vyjimku] //argv(0) - ktery mesto, argv(1) - na ktery linknout
