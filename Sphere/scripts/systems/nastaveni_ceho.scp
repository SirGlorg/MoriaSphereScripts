[function f_nastav_allarmors]
//<argv(0)>	propertyname
//<argv(1)>	class property / variable
arg(u,0)
while (arg(u)<51)
 arg(metaldruh,"<safe n_armor[arg(u)]>")
 if (strcmpi(,<arg(metaldruh)>))
  arg(string,<onestring2(<arg(metaldruh)>)>)
  arg(value,<?safe <tag.nastaveni_prefvalue><arg(string)>?>)
  f_nastav_armorset(<arg(metaldruh)>,<argv(0)>,<arg(value)>,<argv(1)>)
 endif 
 //smsg <arg(metaldruh)>
 arg(u,<arg(u)>+1)
endwhile

[function splitresources]
if (strlen(<argv(1)>))
	singleparse(<argv(0)>)
	var(frnumb,<firstnumb>)
	var(frstr,<secondnumb>)
	singleparse(<argv(1)>)
	var(secnumb,<firstnumb>)
	var(secstr,<secondnumb>)
else
	singleparse(<argv(0)>)
	var(frnumb,<firstnumb>)
	var(frstr,<secondnumb>)
	var(secnumb,0)
	var(secstr,0)
endif

[function f_nastav_weapons_armors_reqstr]
arg(u,35)
while (arg(u)<44)
 arg(v,0)
 while (arg(v)<34)
  arg(value,<eval (<nastaveni_REQSTR_<n_weapons_armors[<arg(v)>]>>*<nastaveni_REQSTR_<n_weapons_armors[arg(u)]>>)/1000>)
  arg(defname,<n_weapons_armors[arg(v)]>_<n_weapons_armors[arg(u)]>)
  if (<?safe.<arg(defname)>?>)//dana vec existuje
   arg(defproperty,<arg(defname)>.REQSTR)
   if (<?eval <arg(defproperty)>?>!=<?arg(value)?>)
    accmsg("Uplatneni hodnoty:'<arg(defproperty)>=<?arg(value)?>'")
    <arg(defproperty)>="<?arg(value)?>"
   endif
  endif
  arg(v,<arg(v)>+1)
 endwhile
 arg(u,<arg(u)>+1)
endwhile

arg(u,35)
while (arg(u)<44)
 arg(v,45)
 while (arg(v)<50)
  arg(value,<eval (<nastaveni_REQSTR_<n_weapons_armors[arg(v)]>>*<nastaveni_REQSTR_<n_weapons_armors[arg(u)]>>)/1000>)
  f_nastav_armorset(<n_weapons_armors[arg(u)]>,<n_weapons_armors[arg(v)]>,reqstr,<arg(value)>,5)
  arg(v,<arg(v)>+1)
 endwhile
 arg(u,<arg(u)>+1)
endwhile

[function f_nastav_weapons_armors_resources]
arg(u,35)
while (arg(u)<44)
 arg(v,0)
 while (arg(v)<34)
  arg(value,<eval (<nastaveni_resources_<n_weapons_armors[<arg(v)>]>>*<nastaveni_resources_<n_weapons_armors[arg(u)]>>)/1000>)
  arg(defname,<n_weapons_armors[arg(v)]>_<n_weapons_armors[arg(u)]>)
  if (<?safe.<arg(defname)>?>)//dana vec existuje
   arg(defproperty,<arg(defname)>.resources)
   //logmsg("<arg(defname)>.resources:<?<arg(defname)>.resources?>")
   splitresources(<?<arg(defproperty)>?>)
   if (<eval <secnumb>>)
    arg(bothvals,<eval <frnumb>+<secnumb>>)
    arg(firstvalue,<eval (((<arg(value)>*1000)/<arg(bothvals)>)*<frnumb>)/1000>)
    arg(secondvalue,<eval <arg(value)>-<arg(firstvalue)>>)
    arg(string,"<arg(firstvalue)> <frstr>, <arg(secondvalue)> <secstr>")
   else
    arg(firstvalue,<arg(value)>)
    arg(string,"<arg(firstvalue)> <frstr>")
   endif
   if (<?eval <arg(defproperty)>?>!=<?arg(firstvalue)?>)
    accmsg("Uplatneni hodnoty:'<arg(defproperty)>=<?arg(string)?>'")
    <arg(defproperty)>="<?arg(string)?>"
   endif
  endif
  arg(v,<arg(v)>+1)
 endwhile
 arg(u,<arg(u)>+1)
endwhile

arg(u,35)
while (arg(u)<44)
 arg(v,45)
 while (arg(v)<50)
  arg(value,<eval (<nastaveni_resources_<n_weapons_armors[arg(v)]>>*<nastaveni_resources_<n_weapons_armors[arg(u)]>>)/1000>)
  f_nastav_armorset(<n_weapons_armors[arg(u)]>,<n_weapons_armors[arg(v)]>,resources,<arg(value)>,3)
  arg(v,<arg(v)>+1)
 endwhile
 arg(u,<arg(u)>+1)
endwhile
