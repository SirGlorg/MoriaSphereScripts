[function f_nastav_armorset]
//<argv(0)>	metal
//<argv(1)>	druh
//<argv(2)>	property
//<argv(3)>	value
//<argv(4)>	
//	0 - class property, 
//	1 - layer armor percentage dependent class property, 
//	2 - variable , 
//	3 - like 1 + stringrest
//	5 - like 0, bez uprav pro stit

//used:
//0 - armor
//1 - weight
//2 - OM
//3 - resources
//5 - reqstr
arg(u,<f_<argv(1)>defname(0,<argv(0)>)>)
while (arg(u)>0)
 arg(defname,<f_<argv(1)>defname(<arg(u)>,<argv(0)>)>)
 arg(value,<argv(3)>)
 if (<argv(4)>!=5)&&(<argv(4)>!=3)
  if (<?eval <safe.<arg(defname)>.type>?>==t_shield)
   arg(value,<argv(3)>/6)
   if (!argv(4))
    if (<arg(defname)>.tdata3==armortype_plate)
     arg(value,<eval arg(value)+nastaveni_global_plateshield_armor_decrease>)//postih pro armor stitu plate brneni (heather shield)
    endif
   endif
  endif
 endif
 if (<argv(4)>==1)||(<argv(4)>==3)
