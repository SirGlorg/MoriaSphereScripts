[ITEMDEF i_gate_timer_marek1]
ID=i_moongate_blue
TYPE=t_normal
CATEGORY=Dungy
SUBSECTION=brany
DESCRIPTION=pusti pocet lidi nebo timer

ON=@Create
attr=attr_newbie
attr=attr_move_never
tag.mytimer=10
tag.players=5
tag.Step=0
tag.timeblock=600

ON=@UserDClick
if (src.isGM)
  dialog(d_guarditAdv)
endif

ON=@Step
if (src.NPC)
  return 1
endif
if (src.isGM)
  src.go(<morep>)
  return 1
endif
if (f_guarditAdv_CheckSpawns)	// 1 == guard is alive
  src.sysMessage("Neprojdes, dokud je strazce pruchodu na zivu.")
  return 1
endif

arg(timeStart,<eval (<serv.time>+(<tag.mytimer>*10))>) // sets the timeStart arg to the value of the current serv.time plus the myTimer, which sets the time hole for all the friendly players to enter the gate (to the maximum of tag.players players)
tag(Step,<tag(Step)>+1) //increases number of players stepped on the gate since last opening.

if (<tag.Step> == 1) //first player to open the gate entering sets the tag.time to the timerStart value.
  tag.time=<arg(timestart)> 
endif

if ((<tag.time> > <serv.time>)&&(<tag.Step> < <tag.players>)) //if the time is in the gap between first player enter time and tag.mytimer seconds after, than player is allowed in the gate (the maximum number of players is also checked)
  src.go <morep>
else
  src.sysMessage("Pruchod je uzavren.")
endif

if ((<tag.time> + (<tag.timeblock>*10)) < <serv.time>) // if the time is over the time gap plus the tag.timeblock seconds, than the stepping player resets the gate properties and allows himself in the gate in the next @step trigger round.
  tag.time=0
  tag.Step=0
endif
return 1



