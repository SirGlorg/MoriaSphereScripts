[function equiptag]
arg(MyEquipRune,<findid(i_playerEquip_rune)>)
if !(ischar)
  return""
endif
if (<argvcount> == 2)
  if !(<findid(i_playerEquip_rune)>) // postava jeste nema quest runu
    newequip(i_playerEquip_rune)
    arg(MyEquipRune,<lastnew>)
  endif
  MyEquipRune.tag(<argv(0)>,"<argv(1)>")
  return""
elseif (<argvcount> == 1)
  if !(<findid(i_playerEquip_rune)>)
    return""
  endif
  if (strlen(<MyEquipRune.tag(<argv(0)>)>))
    return<MyEquipRune.tag(<argv(0)>)>
  else
    return""
  endif
endif
RedMessage("Spatny pocet argumentu.")
Return""

