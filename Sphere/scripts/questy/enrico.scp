[dialog d_spawnpad_help]
argo.settext(1," ID monstra: sem vepis ID mocnstra, ktere se ma po naslapnuti spawnout.<br><br> Respawn/ds: cas, po kterem lze naslapku znovu aktivovat po predchozim uspesnym vyslapnuti NPC definovaneho v radku ID monstra.<br><br> Zmizeni/s: Vyslapnuta NPC budou automaticky odstranena po ubehnuti tohoto timeru. Cas je nutne nastavit VZDY! Pokud tak neucinite, bude automaticky nastaven na hodnotu 900s, tedy 15 minut.<br><br> Sance/%: hodnota v % udavajici pravdepodobnost, s jakou hrac aktivuje spawn po uspesnem seslapnuti. Sance MUSI byt nastavena, v opacnem pripade ji bude prirazena defaultni hodnota 30%.<br><br> RemoveTrap & MoveTrap: univerzalni funkce. Popis v jejich dialogu po stistknuti prislusneho tlacitka.")
argo.f_dialog_HelpCreator("HELP - SpawnPad nastaveni")

[function f_pad_spawnPad_setDialogValues]
tag(spawnID,<argv(0)>)
if (<eval argv(1)> > 0)
 tag(restime,<eval argv(1)>)
else
 tag(restime,450)
 src.RedMessage("Pozor! <src.sex(Nenastavil,Nenastavila)> jsi cas pro respawn! Automaticky prirazeno 45s")
endif
if (<eval argv(2)> > 0)
  tag(npcDisappearTime,<eval argv(2)>)
else
  tag(npcDisappearTime,900)
  src.RedMessage("Pozor! <src.sex(Nenastavil,Nenastavila)> jsi cas pro respawn! Automaticky prirazeno 900s")
endif
if (<eval argv(3)> > 0)
  tag(spActivateChance,<eval argv(3)>)
else
  tag(spActivateChance,300)
  src.RedMessage("Pozor! <src.sex(Nenastavil,Nenastavila)> jsi sanci na aktivaci! Automaticky prirazeno 30%")
endif

