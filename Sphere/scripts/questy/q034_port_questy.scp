  if (strlen(<tag.timeragainall>))
    if (eval(<tag(again)>) > <serv.time>)
      say("JESTE TAM NEMUZES") //nemoze nikto
      return 1
    endif
    tag(again,<serv.time>+<tag.timeragainall>*10)
  endif
  
