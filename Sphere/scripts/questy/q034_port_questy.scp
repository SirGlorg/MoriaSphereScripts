      say("UZ SI QUEST UROBIL UZ NEMOZES")
      return 1
    endif
  endif
  if (<tag.canrepeat>==1)
    if (strlen(<tag.timeragain>))
      if (eval(<src.qtag(q034_<tag.questid>_again)>) > <serv.time>)
