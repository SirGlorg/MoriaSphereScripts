      src.qtag(q034_<tag.questid>_againafterdeny,<serv.time>+<tag.timeragainafterdeny>*10)
    endif
  endif
  src.qtag(q034_<tag.questid>_canrepeatafterdeny,<tag.canrepeatafterdeny>)

  say("Toto povie npc ak odmietne")
endif
