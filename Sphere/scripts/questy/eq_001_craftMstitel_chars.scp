[function f_eq_cm_restart_boss]//arg = uid spawnu bossa
if (<finduid(<argv>).tag(guard_1)>)
  arg(guard,<finduid(<argv>).tag(guard_1)>)
  if (<finduid(argv).tag(guard_1).more2> == 01)
    finduid(<arg(guard)>).tag(lastspawned).remove
    finduid(<arg(guard)>).timer=1
  else
    finduid(<arg(guard)>).timer=1
  endif
endif
if (<finduid(<argv>).tag(guard_2)>)
  arg(guard,<finduid(<argv>).tag(guard_2)>)
  if (<finduid(argv).tag(guard_2).more2> == 01)
    finduid(<arg(guard)>).tag(lastspawned).remove
    finduid(<arg(guard)>).timer=1
  else
    finduid(<arg(guard)>).timer=1
  endif
endif
if (<finduid(<argv>).tag(guard_3)>)
  arg(guard,<finduid(<argv>).tag(guard_3)>)
  if (<finduid(argv).tag(guard_3).more2> == 01)
    finduid(<arg(guard)>).tag(lastspawned).remove
    finduid(<arg(guard)>).timer=1
  else
    finduid(<arg(guard)>).timer=1
  endif
endif
if (<finduid(<argv>).tag(guard_4)>)
  arg(guard,<finduid(<argv>).tag(guard_4)>)
  if (<finduid(argv).tag(guard_4).more2> == 01)
    finduid(<arg(guard)>).tag(lastspawned).remove
    finduid(<arg(guard)>).timer=1
  else
    finduid(<arg(guard)>).timer=1
  endif
endif

if (<finduid(<argv>)>)
  arg(boss,<finduid(<argv>)>)
  if (<finduid(argv).more2> == 01)
    finduid(<arg(boss)>).tag(lastspawned).remove
    finduid(<arg(boss)>).timer=1
  else
    finduid(<arg(boss)>).timer=1
  endif

endif

