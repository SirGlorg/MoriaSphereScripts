//boss
[EVENTS e_eq_cm_mocichtivy_zbohatlik]
on=@aftergetswing
if (<flag_invul>==1)//ak nahodou ostal
  flag_invul=0
endif

//na dvourucky (lba a podobne)  aby mel cca 95% sanci ze tu  ranu vrati normal  a cca 33% ze vrati ale s dvojnasob dmg
if (<act.findlayer(2).typedef.twohands>)  //ak udrej s dvojruckou
  if (90 > {0 100})
    if (33 > {0 100})
      src.damage(2*<var(ubrano)>,<tag(weapflags)>)
    else
      src.damage(<var(ubrano)>,<tag(weapflags)>)
    endif
  endif
endif

if (<tag.stage> == 0) // este neni ziadny strazca volny
  if (<hits> < (<maxhits>/100)*80)
    tag.stage=1
  endif
endif

if (<tag.stage>==1)
  if (<hits> < (<maxhits>/100)*60)
    tag.stage=2
  endif
endif

if (<tag.stage>==2)
  if (<hits> < (<maxhits>/100)*40)
    tag.stage=3
  endif
endif

if (<tag.stage>==3)
  if (<hits> < (<maxhits>/100)*20)
    tag.stage=4
  endif
endif

  if (<memoryfindtype(MEMORY_ISPAWNED).link.tag(guard_<tag(stage)>)>) // ak je zadany spawn strazcu
    arg(strazca, <memoryfindtype(MEMORY_ISPAWNED).link.tag(guard_<tag(stage)>).tag(lastspawned)>)
    if (<memoryfindtype(MEMORY_ISPAWNED).link.tag(guard_<tag(stage)>).more2>==1)  //ak je strazca spawnnuty/zije
    
      if (<finduid(strazca).tag(stoned)>==1)
        finduid(strazca).stone=0
        finduid(strazca).flag_invul=0
        finduid(strazca).karma={-8000 -10000}
        finduid(strazca).resync
        finduid(strazca).tag(stoned,0)
        finduid(strazca).update

      //negovanie spawnov (nastavenie na max timer) // maxtime = na spawne v morey
        finduid(memoryfindtype(MEMORY_ISPAWNED).link).timer = <memoryfindtype(MEMORY_ISPAWNED).link.morey>*60
        finduid(memoryfindtype(MEMORY_ISPAWNED).link.tag(guard_1)).timer=<finduid(memoryfindtype(MEMORY_ISPAWNED).link.tag(guard_1)).morey>*60
        finduid(memoryfindtype(MEMORY_ISPAWNED).link.tag(guard_2)).timer=<finduid(memoryfindtype(MEMORY_ISPAWNED).link.tag(guard_2)).morey>*60
        finduid(memoryfindtype(MEMORY_ISPAWNED).link.tag(guard_3)).timer=<finduid(memoryfindtype(MEMORY_ISPAWNED).link.tag(guard_3)).morey>*60
        finduid(memoryfindtype(MEMORY_ISPAWNED).link.tag(guard_4)).timer=<finduid(memoryfindtype(MEMORY_ISPAWNED).link.tag(guard_4)).morey>*60
      endif
      
      if (finduid(strazca).flag_invul==1)//ak nahodou ostal
        finduid(strazca).flag_invul=0
      endif
      
      hits=<hits>+<var(ubrano)>*1.2 //ak strazca zije nejde uberat bossovy    //ak zije ten vypusteny strazca tak nezranuje bossa
    endif
    

  endif
  
say("<tag.stage>")
return(0)

