    argo.setText(1,"Borek te pozadal, aby jsi <?src.sex("odmontoval","odmontovala")?> radlici z pluhu a <?src.sex("donesl","donesla")?> ji opravit.")
  elseif (<eval src.qtag(mq002)> == 2)
    argo.setText(1,"Uz vis, jak vybabrat radlici z Borkova pluhu. Je na case to udelat.")
  elseif (<eval src.qtag(mq002)> == 3)
