[function f_qDialogInstructions]	//argv(0) -> ksichtik; argv(1) -> nazev gumpu; argv(2)&argv(3) -> x&y ksichtik position
SetLocation(5,5)
page 0
resizepic(20,20,2620,400,202)
if (argvcount > 1)
  text(195,40,32,<eval argv(1)>)  // nazev gumpu
else
  text(195,40,32,0)  // nazev gumpu
endif
gumppic(25,30,03182)		// podklad pod ksichtiky	
if (argvcount > 2)
  gumppic(<argv(2)>,<argv(3)>,<argv(0)>)	// ksichtik (03000-03164)
else
  gumppic(36,37,<argv(0)>)	// ksichtik (03000-03164)
endif
button(384,187,9906,9908,1,0,1)	// vraci zpet do zakladni nabidky


[function f_questMenu_getName]
return<argv(0)?>

[function f_questMenu_getDialog]
return<argv(1)>

[function f_questMenu_hasRelevantTag]
arg(tagCount,<argvcount>-3)
arg(tagNumber,3)
arg(cycleArg,0)
if (<argv(2)>)
  while (<cycleArg> < <tagCount>)
    if (0<src.qtag(<argv(<tagNumber>)>)> !=0)
      return 1
    else
      arg(tagNumber,#+1)
      arg(cycleArg,#+1)
    endif
  endwhile
else
  while (<cycleArg> < <tagCount>)
    if (0<src.tag(<argv(<tagNumber>)>)> !=0)
      return 1
    else
      arg(tagNumber,#+1)
      arg(cycleArg,#+1)
    endif
  endwhile
endif
return 0

[function f_questMenu_reportQuest]
arg(relevantDialogName,<f_questMenu_getDialog(<questMenu[<argv(0)>]>)>)
sfx(513)
dialog(d_questMenu_reportDialog,<arg(relevantDialogName)>)

[dialog d_questMenu_reportDialog]
argo.NoMove=1
argo.NoClose=1
argo.SetLocation=5,30
argo.tag(relevantQuestDialog,<argv(0)>)
button(0,0,01e,01e,1,0,1) // otevre dialog prislusneho questu
//argo.tilepichue(11,70,<hcn_clueIconA[<eval more2>]>)

[dialog d_questMenu_reportDialog button]
on=1 // open quest dialog
dialog(<argo.tag(relevantQuestDialog)>)

