[SPEECH spk_q007_haradir]
ON = *hi*
ON = *dobry*
ON = *ahoj*
ON = *hospodsky*
ON = *selatko*

if (<distance>>3)
 src.SYSMESSAGE="Na takovou vzdalenost s tebou jednat nebude"
else
 f_q007_haradir_DClick
endif



///////////// ON DCLICK+SPEECH Haradir Function

[FUNCTION f_q007_haradir_DClick]
src.f_q007_checkOldTags
if (0<src.qtag(q007_haradirovo_selatko)> == 8)
  DIALOG(d_q007_haradir8)
elseif ((0<src.qtag(q007_haradirovo_selatko)> == 7) && (<SRC.RESTEST 1 i_selatko>))
  DIALOG(d_q007_haradir7)  
elseif (0<src.qtag(q007_haradirovo_selatko)> == 7)
  DIALOG(d_q007_haradir6)
elseif (0<src.qtag(q007_haradirovo_selatko)> >= 5)
  DIALOG(d_q007_haradir5)
elseif (0<src.qtag(q007_haradirovo_selatko)> >= 3)
  DIALOG(d_q007_haradir4)
elseif (0<src.qtag(q007_haradirovo_selatko)> == 2)
  DIALOG(d_q007_haradir3)
elseif (0<src.qtag(q007_haradirovo_selatko)> == 1)
  DIALOG(d_q007_haradir2)
else 
  DIALOG(d_q007_haradir)
endif


