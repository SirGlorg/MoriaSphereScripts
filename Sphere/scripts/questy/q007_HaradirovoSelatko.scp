argo.page(9)
if (<eval <src.qtag(q007_haradirovo_selatko)>> > 6) && !(<eval src.qtag(q017_terlak_ochrana)>) && !(<eval src.qtag(q017_terlak)>)
  argo.setText(25,"*Hospodsky se na tebe uprene podival* Nezminoval jsem se, ze je ten lektvar v podstate k nicemu ? Proti utocnemu kouzlu neobstoji a divil bych se, kdyby jsi poznal rozdil... Zvlastni asi to vsichni maji potrebu zkusit... Ten hlupak Gargamel byl tim lektvarem take posedly a jeste pred odjezdem mi za jednu davku zaplatil celych tisic zlatych. Nedivil bych se, kdyby to byla jeho plna mzda za cestu k Terlakovi.. Ze jsi to ty, jednu davku ti namicham zdarma, stale ti to dluzim. Aby byl ucinek platny, musis lektvar vypit jeste horky. Dokud se s obrannym kouzlem nestretnes, bude lektvar pusobit. Nemysli si ale, ze te jeho ucinky ochrani podruhe. Jak jednou zapusobi, telo jej vyplavi a je potreba dat si novou davku.")
  argo.setText(26,"*Pockat nez hospodsky pripravi lektvar a napit se...*")
  argo.HTMLGUMP(170,230,440,100,25,0,36)
  argo.button(25,395,0fa5,0fa7,1,0,8)	// napit se ochranneho lektvaru -> Terlak Gate
  argo.text(60,398,142,26)
elseif (<eval <src.qtag(q007_haradirovo_selatko)>> == 1)		// jen blbec mi neda sele
  argo.setText(25,"*Dlouze se na tebe zahledel* ... Vidis tady ten pult? Diky nemu ti mohu podat cokoliv si obednas, aniz bych se te jen lehce dotkl. Tady zamnou muzes spatrit polici plnou cistych sklenic a poharu, ktere ti naplnim cimkoliv si jen budes prat. A tady nahore nejvyzsi chlouba me male hospudky. Visi zde velka popsana cedule, kde je vypsano vse, co ti mohu nabidnout. Kdyz na ni pohlednes co vidis ? PROTOZE JEN NEGRAMOTNY HLUPAK TAM VIDI NEJAKE SELE!")
  argo.setText(33,"*zvednout svicen ze stolu*")
  argo.HTMLGUMP(170,230,440,100,25,0,36)
  argo.button(25,395,0fa5,0fa7,0,7,0)
  argo.text(60,398,152,20)
  argo.button(25,415,0fa5,0fa7,0,13,0)
  argo.text(60,418,142,33)
  argo.button(25,435,0fa5,0fa7,1,0,5)
  argo.text(60,438,142,30)
elseif  (<eval <src.qtag(q007_haradirovo_selatko)>> == 5)	// chce abych se naklonil s vetsim durazem
