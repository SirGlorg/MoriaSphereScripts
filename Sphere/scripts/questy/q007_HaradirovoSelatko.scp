argo.page(8)
if (<eval <src.qtag(q007_haradirovo_selatko)>> > 6) && !(<eval src.qtag(q017_terlak_ochrana)>) && !(<eval src.qtag(q017_terlak)>)
  argo.setText(23,"Dlouho jsem o nem neslysel, az ten opilec vcera tvrdil, ze ma s Terlakem schuzku... Drzel v ruce velmi zajimave krystaly, porad si s nimi pohraval a tvrdil, ze jsou to klice... <?src.sex(veril,verila)?> by jsi tomu? Navic hledal zoldaka, ktery by mu pomohl dostat se do jeskyni kdesi severne od Belfalske zatoky... Nakonec se mu podarilo za par mescu zlatych najmout Gargamela, znameho a neprilis prijemneho zoldaka.")
  argo.setText(24,"Mohl by jsi mi pripravit ten ochranny lektvar?")
  argo.HTMLGUMP(170,230,440,100,23,0,36)
