// popis zmenenych tagu:
//Selatko == 1 Zadan ukol haradirem
//Lakomec == 1 Hospodsky odmita vydat sele, tvrdi ze nic nema
//Carodejnice == 1 Haradir me rek, ze by mohla pomoct carodejka
//Carodejnice_regy == 1 Carodejnice chce donyst regy aby pomohla se seletem
//Rachel == 1 Dal sem Rachel regy a ona mi za to slibila pomoc s hospodskym.
//Rachel == 2 Hospodskej me posila pro Luk Dryady jako dukaz, ze sem ji zabil
//Informace == 1 Hospodskej mi konecne dal to sele
//Hotovo == 1 -> remove tagu: selatko rachel informace lakomec carodejnice
//Kovar == 1 -> zadan ukol kovarem - mam donyst 3 sutry
//Kovar == 2 -> dones sem sutry a 
//Kniha == 1 -> zadanej ukol inscripterem donyst sutry
//Kniha == 2 -> mam knihu inscripce
//Kniha == 3 -> vymenil sem knihu za pero



[function f_q007_checkOldTags]
if (0<src.tag(selatko)> == 1)
  src.qtag(q007_haradirovo_selatko,1)
  src.tag.remove(selatko)
endif
if (0<src.tag(Lakomec)> == 1)
  src.qtag(q007_haradirovo_selatko,2)
  src.tag.remove(Lakomec)
endif
if (0<src.tag(Carodejnice)> == 1)
  src.qtag(q007_haradirovo_selatko,3)
  src.tag.remove(Carodejnice)
endif
if (0<src.tag(Carodejnice_regy)> == 1)
  src.qtag(q007_haradirovo_selatko,4)
  src.tag.remove(Carodejnice_regy)
endif
if (0<src.tag(Rachel)> == 1)
  src.qtag(q007_haradirovo_selatko,5)
  src.tag.remove(Rachel)
elseif (0<src.tag(Rachel)> == 2)
  src.qtag(q007_haradirovo_selatko,6)
  src.tag.remove(Rachel)
endif
if (0<src.tag(Informace)> == 1)
  src.qtag(q007_haradirovo_selatko,7)
  src.tag.remove(Informace)
endif
if (0<src.tag(Hotovo)> == 1)
  src.qtag(q007_haradirovo_selatko,8)
  src.tag.remove(Hotovo)
endif
if (0<src.tag(Kovar)> != 0)
  src.qtag(q007_prastary_kovar,<src.tag(Kovar)>)
  src.tag.remove(Kovar)
endif
if (0<src.tag(Kniha)> != 0)
  src.qtag(q007_stary_inscripter,<src.tag(Kniha)>)
  src.tag.remove(Kniha)
endif
if (0<src.tag(q007_haradir_Nemethen)> == 1)
  src.qtag(q007_haradir_Nemethen,1)
  src.tag.remove(q007_haradir_Nemethen)
endif

[function f_q007_Rodrigo_checkOldTags]
if (<src.tag.Rodrigo> == 1)
  src.qtag(q007_Rodrigo,1)
  src.tag.remove(Rodrigo)
endif
if (<src.TAG.q007_delfin> == 1)
  src.qtag(q007_delfin,1)
  src.tag.remove(q007_delfin)
endif


