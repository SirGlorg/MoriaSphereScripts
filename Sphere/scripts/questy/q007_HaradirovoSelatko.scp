  argo.setText(40,"K cemu ? ! *Hospodsky vytrestene vyhrkl* ... Nejsem si jisty, jestli to chci vubec vedet ... A hlavne si nejsem jisty, zda si uvedomujes skutecnou cenu toho lektvaru. Jeden takovy lektvar vyzaduje desitky ingredienci a nektere jsou velice vzacne !")
  argo.setText(41,"Je mi lito, ale skutecne potrebuji ten lektvar.")
