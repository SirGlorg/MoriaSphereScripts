  argo.text(60,418,142,30)
elseif  (<eval <src.qtag(q007_haradirovo_selatko)>> == 5) // chce abych se naklonil
  argo.setText(24,"Tohle se nemelo nikdy stat... Poslouchej mne ted dobre... *hospodsky se nervozne rozhledl, naklonil se bliz a naznacil, ze chce totez po tobe*...")
  argo.setText(25,"Poslouchej, na tyhle komedie nemam cas...")
